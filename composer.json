{
    "name": "onyok/website",
    "description": "A website for Dan Rose Fishing",
    "type": "project",
    "require": {
        "php": "^8.2.12",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "guzzlehttp/guzzle": "^7.0",
        "monolog/monolog": "^2.0",
        "pear/http_request2": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
